  "compilerOptions": {
    "composite": true,
    "outDir": "./storybook-dist",
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true
  },
  "exclude": [
    "scripts"
  ]
}