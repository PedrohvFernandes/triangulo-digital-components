  "name": "triangulo-digital-components",
  "description": "Triângulo Digital Components Library - Design System do Triângulo Digital",
  "repository": {
    "type": "git",
    "url": "https://github.com/PedrohvFernandes/triangulo-digital-components"
  },
  "author": {
    "name": "Pedro Henrique V. Fernandes",
    "email": "pedrohv20fernandes@gmail.com"
  },
  "keywords": [
    "DS",
    "Design System",
    "React",
    "Components"
  ],
  "homepage": "https://github.com/PedrohvFernandes/triangulo-digital-components#readme",
  "license": "UNLICENSED",
  "version": "0.5.7",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/triangulo-digital-components.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/triangulo-digital-components.umd.js"
      }
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    },
    "./raw.css": {
      "require": "./dist/src/raw.css",
      "default": "./dist/src/raw.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "generate:index": "ts-node scripts/generate-exports.ts",
    "build:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./dist/styles.css",
    "build:raw-css": "cpy ./src/tailwind-entry.css ./dist --rename=raw.css",
    "build:scripts": "tsc --project tsconfig.scripts.json",
    "build": "npm run generate:index && tsc && vite build && npm run build:css && npm run build:raw-css && npm run build:scripts",
    "lint": "eslint . --cache --report-unused-disable-directives && echo \"eslint: no lint errors found!\"",
    "lint:fix": "eslint . --cache --fix && echo \"eslint: no lint errors found!\"",
    "prepublishOnly": "npm run build",
    "dev:storybook": "concurrently \"npm run storybook:css\" \"storybook dev -p 6006\"",
    "storybook:css": "tailwindcss -w -i ./src/tailwind-entry.css -o ./src/index.css",
    "start:storybook": "concurrently \"npm run build-storybook\" \"serve storybook-dist -l 6006\"",
    "build-storybook": "concurrently \"npm run build-storybook:css\" \"storybook build -o storybook-dist\"",
    "build-storybook:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./src/index.css",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react-dom": "^19.1.1",
    "react": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "inquirer": "^12.9.6"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": false
    },
    "inquirer": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "inquirer": "^12.9.6"
  },
  "devDependencies": {
