  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "generate:index": "ts-node scripts/generate-exports.ts",
    "build:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./dist/styles.css",
    "build:fontcss": "tailwindcss -m -i ./src/fonts.css -o ./dist/fonts.css",
    "build:raw-css": "cpx ./src/tailwind-entry.css ./dist/tailwind-entry.css",
    "build:scripts": "tsc --project tsconfig.scripts.json",
    "build": "npm run generate:index && tsc && vite build && npm run build:css && npm run build:raw-css && npm run build:fontcss && npm run build:scripts",
    "lint": "eslint . --cache --report-unused-disable-directives && echo \"eslint: no lint errors found!\"",
    "lint:fix": "eslint . --cache --fix && echo \"eslint: no lint errors found!\"",
    "prepublishOnly": "npm run build",
    "dev:storybook": "concurrently \"npm run storybook:css\" \"storybook dev -p 6006\"",
    "storybook:css": "tailwindcss -w -i ./src/tailwind-entry.css -o ./src/index.css",
    "start:storybook": "concurrently \"npm run build-storybook\" \"serve storybook-dist -l 6006\"",
    "build-storybook": "concurrently \"npm run build-storybook:css\" \"storybook build -o storybook-dist\"",
    "build-storybook:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./src/index.css",
    "prepare": "husky"
  },
